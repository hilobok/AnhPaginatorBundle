{
    "name": "anh/paginator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony integration for anh/paginator library",
    "keywords": [
        "pager", "paginator", "pagination", "paging"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Hilobok",
            "email": "hilobok@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.4",
        "anh/paginator": "0.2.*",
        "symfony/framework-bundle": "~2.5",
        "symfony/assetic-bundle": "~2.3"
    },

    "minimum-stability": "stable",

    "autoload": {
        "psr-4": {
            "Anh\\PaginatorBundle\\": ""
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}

